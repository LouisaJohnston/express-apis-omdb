<% // Render your search results here %>
<% // This is how to do comments in EJS, by the way... %>
<h1>Search results</h1>
<% movies.forEach(movie => { %>
    <h3><%= movie.Title %></h3>
    <img src="<%= movie.Poster %>" width=200px />
    <a href="/detail/<%= movie.imdbId %>">SHOW ME DETAILS</a>
<% }) %>